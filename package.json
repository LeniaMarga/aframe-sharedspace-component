{
  "name": "aframe-chat",
  "version": "1.0.0",
  "description": "VR chat powered by A-Frame",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "clean": "shx rm -rf site",
    "copy": "shx cp -r index.html vendor assets dist site/",
    "deploy": "npm run build && npm run copy && gh-pages -d site",
    "postdeploy": "npm run clean",
    "predeploy": "npm run clean && shx mkdir -p site",
    "start": "webpack-dev-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aframevr",
    "vr",
    "skeleton",
    "template",
    "boilerplate"
  ],
  "author": "Salvador de la Puente <salva@unoyunodiez.com>",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "shx": "^0.2.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "aframe": "^0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delapuente/aframe-chat.git"
  }
}
